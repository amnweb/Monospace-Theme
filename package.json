{
    "name": "monospace-idx-theme",
    "displayName": "Monospace IDX Theme",
    "description": "Monospace theme for VSCode",
    "publisher": "SpaceBox",
    "license": "MIT",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.83.0"
	},
    "repository": {
